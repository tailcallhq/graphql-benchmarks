# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: Int!
  name: String!
  username: String!
  email: String!
  posts: [Post!]!
}

type Post {
  userId: Int!
  id: Int!
  title: String!
  body: String!
  user: User
}

type Query {
  users: [User!]!
  posts: [Post!]!
}

